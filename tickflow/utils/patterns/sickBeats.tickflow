0x105 
if 0
	goto 0x6F
endif 
0x103 0x30
rest 0x30	// 1.0 beats
0x104 
rest 0x30	// 1.0 beats
0x105 
if 0
	goto 0x6F
endif 
0x103 0x30
rest 0x30	// 1.0 beats
0x104 
rest 0x30	// 1.0 beats
0x105 
