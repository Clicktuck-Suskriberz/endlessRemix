// Doctor's beat anims starts automatically.

sequenceSickBeats:
	set_cat 0
    rest 0x8
	call changeEngine
	engine 0x40
	sub 4
	0x29<2> 
    rest 0x10
	0xB3 9, 0x40
    fade<1> 1,1,1
    input 1
    0xAF<6> 0
    random 1
    switch
    case 0
		0xB3 13, 14
        async_call sickBeats1
    endswitch
    async_call sickBeatsBeatAnims
    0x10A 0
	0x109 1
stop

sickBeats1:
0x10C
async_call sickBeats1Virus1
async_sub 0x5C
rest 0xC0
async_sub 0x5B
rest 0x30
async_sub 0x58
rest 0x90
async_sub 0x59
stop

sickBeats1Virus1:
0x100 
0x101 0
0x102 
0x102 
