    rest 0x18
    0x100 0
    rest 0x30
    0x100 0
    0x102
    rest 0x60
    rest 0xA8
    0x105 0
    rest 0x18
    
    0x101
    0x100 3
    rest 0x48
    0x100 0
    rest 0x18
    0x102
    rest 0x18
    0x100 1
    rest 0x18
    0x100 0
stop

