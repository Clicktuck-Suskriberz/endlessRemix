// Nothing special here.

sequenceTweezers:
	set_cat 0
    rest 0x8
	call changeEngine
	engine 0x3
	sub 4
	0x29<2> 
    rest 0x10
	0xB3 engine_loaded, 0x3
    fade<1> 1,1,1
    input 1
    random 2
    switch
    case 0
        0xB3 score_add, 8
        call tweezers1
        break
    case 1
        0xB3 score_add, 8
        call tweezers2
        break
    endswitch
stop

