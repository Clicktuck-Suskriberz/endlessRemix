// Starts with Captain Tuck present.

preSequenceFlipperFlop:
rest 0x18
play_sfx 0x1000884
rest 0x18	// 0.5 beats
0x40<1> 0x1000291, 0x100
rest 0x30	// 1.0 beats
0x40<1> 0x1000291, 0x100
rest 0x30	// 1.0 beats
0x40<1> 0x1000291, 0x100
rest 0x30	// 1.0 beats
play_sfx 0x10008B1
0x40<1> 0x1000291, 0x100
rest 0x30	// 1.0 beats
stop

sequenceFlipperFlop:
	set_cat 0
	call changeEngine
	engine 0x26
	sub 4
	0x29<2> 
    rest 0xC
    0x65<4> 0x12C, 0x3C
    0x65<3> 0, 0x50
    0x65<5> 2
    0x100 0xC, 0
    input 1
    random 3
    switch
    case 0
        0xB3 score_add, 17
        async_call flipperFlop1
        break
    case 1
        0xB3 score_add, 17
        async_call flipperFlop2
        break
    default
        0xB3 score_add, 17
        async_call flipperFlop3
        break
    endswitch
    0x2B<1> 0x26
    if_neq 0xF
        0x105<4>
    endif
    rest 0xC
	0xB3 engine_loaded, 0x26
    fade<1> 1,1,1
stop

flipperFlop1:
rest 0xC
0x103
0x105<1> 
async_call flipperFlopFlip
rest 0x30
async_call flipperFlopFlip
async_call flipperFlopTriple
async_sub 0x63
rest 0x30
async_sub 0x63
rest 0x30
async_sub 0x63
rest 0x30
async_sub 0x63
rest 0x30
async_sub 0x63
rest 0x30
async_sub 0x70
rest 0xC0
async_sub 0x63
rest 0x30
async_sub 0x63

stop

