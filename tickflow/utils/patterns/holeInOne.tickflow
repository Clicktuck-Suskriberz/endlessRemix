golfBeatAnims:
label 0x601F
0x102<1>
0x103<4>
0x104<2>
rest 0x30
goto 0x601F

golfSceneSetup:
0x2B<1> 0x25
if 5
    0x106 "bg_airplane", -1, 0x1800, 0x1E, 2, -0x6E, 0, 0x10, -0x6E, -0x14, 0x100
    0x106 "bg_cloud", 3, 0xC000, 0x1E, 2, -0xAF, -0xA, 0x100, -0x1DB, -0xA, 0x100
    0x106 "bg_cloud", 1, 0xC000, 0x1E, 2, -0x4B, -0x19, 0x100, -0x4B, -0x19, 0x100
    0x106 "bg_cloud", 2, 0xC000, 0x1E, 2, 0, -0xA, 0x100, 0x64, -0xA, 0x100
    0x106 "bg_cloud", 0, 0xC000, 0x1E, 2, 0x96, -0xA, 0x100, 0x64, -0xA, 0x100
endif
stop