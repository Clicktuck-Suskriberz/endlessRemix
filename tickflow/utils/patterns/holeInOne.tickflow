// Has multiple variants, use 0x2B<1> to know.

sequenceGolf:
	set_cat 0
    rest 0x8
	call changeEngine
	engine 0x25
	sub 4
	0x29<2> 
    rest 0x10
    0x101<1> 2
	0xB3 engine_loaded, 0x25
    fade<1> 1,1,1
    input 1
    random 5
    switch
    case 0
        0xB3 score_add, 5
        async_call golf1
        break
    case 1
        0xB3 score_add, 5
        async_call golf2
        break
    case 2
        0xB3 score_add, 5
        async_call golf3
        break
    case 3
        0xB3 score_add, 5
        async_call golf4
        break
    case 4
        0xB3 score_add, 5
        async_call golf5
        break
    endswitch
    async_call golfBeatAnims
    async_call golfSceneSetup
stop

golf1:
async_sub 0x56
rest 0xC0
async_sub 0x56
rest 0x60
async_sub 0x56
rest 0x60
async_sub 0x57
rest 0xC0
async_sub 0x56
stop

