// Nothing quite interesting to report here ;P

presequenceAirRally:
rest 0x48
play_sfx 0x10007C9
rest 0x30
play_sfx 0x10007CA
rest 0x30
play_sfx 0x10007CB
stop

sequenceAirRally:
	set_cat 0
    rest 0x8
	call changeEngine
	engine 0x1D
	sub 4
	0x29<2> 
    rest 0x10
	0xB3 engine_loaded, 0x1D
    0x101<0> 1, 1
    0x101<1> 2, 1
    0x102
    fade<1> 1,1,1
    input 1
    random 5
    switch
    case 0
        0xB3 score_add, 8
        async_call airRally1
        break
    case 1
        0xB3 score_add, 6
        async_call airRally2
        break
    case 2
        0xB3 score_add, 7
        async_call airRally3
        break
    case 3
        0xB3 score_add, 7
        async_call airRally4
        break
    case 4
        0xB3 score_add, 7
        async_call airRally5
        break
    endswitch
stop

