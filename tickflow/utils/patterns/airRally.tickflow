async_call aRNormal
rest 0xC0	// 4.0 beats
async_call aRBadmintonDistance
async_call aRNormal
rest 0xC0	// 4.0 beats
async_call aRBadmintonDistance
async_call aRNormal
rest 0xC0	// 4.0 beats
async_call aRBadmintonDistance
async_call aRNormal
rest 0xC0	// 4.0 beats
stop

