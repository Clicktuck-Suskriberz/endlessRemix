addBirdsBeat:
label 0x6901
random 4
switch
case 0
    0x102<5> 3
    break
case 1
    0x102<5> 5
    break
case 2
    0x102<5> 8
    break
default
    break
endswitch
rest 0x60
goto 0x6901
stop

