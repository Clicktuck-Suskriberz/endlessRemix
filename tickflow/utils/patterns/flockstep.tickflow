// Nothing to report yet.

sequenceFlockstep:
	set_cat 0
    rest 0x8
	call changeEngine
	engine 0x1F
	sub 4
	0x29<2> 
    rest 0x10
    0x102<4>
    0x8B 0, 0
    random 2
    switch
    case 0
        0x107 1
        0x102<5> 30
        break
    default
        0x107 0
        0x102<5> 30
        break
    endswitch
	0xB3 engine_loaded, 0x1F
    fade<1> 1,1,1
    random 2
    switch
    case 0
        0xB3 score_add, 3
        async_call flockstep1
        break
    default
        0xB3 score_add, 3
        async_call flockstep2
        break
    endswitch
    async_call initialBeats
    rest 0x30
    input 1
    rest 0x90
    random 2
    switch
    case 0
        async_call addBirdsBeat
        break
    case 1
        break
    endswitch
stop

