munchyMonkBeat:
label 0x3056
0x101
rest 0x30
goto 0x3056

munchyMonkOneDumpling:
    set_cat 0
    rest 0x1B	// 0.5625 beats
    play_sfx 0x1000C2E
    rest 0x15	// 0.4375 beats
    0x8E 0
    0x100 0
    0x102 0
    0x103 0
    rest 0x18	// 0.5 beats
    0x103<1> 
stop

