// Has 3 starting states,
// 1 dumpling, 2 dumplings and 3 dumplings.
// Place your subs accordingly.

preSequenceMunchyMonk:
rest 0xC0
random 3
switch
case 0
    async_call munchyMonkOneDumpling
    0xB3 game_state, 0
    break
case 1
    async_call munchyMonkTwoDumplings
    0xB3 game_state, 1
    break
case 2
    async_call munchyMonkThreeDumplings
    0xB3 game_state, 2
    break
endswitch
stop

sequenceMunchyMonk:
	set_cat 0
    rest 0x8
	call changeEngine
	engine 0x1B
	sub 4
	0x29<2> 
    rest 0x8
    0x106<1>
    0x104<1>
    0x107 1, 0xC0
    rest 0x8
    async_call munchyMonkBeat
	0xB3 9, 0x1B
    fade<1> 1,1,1
    0xB2 game_state
    switch
    case 0
        random 1
        switch
        case 0
            0xB3 score_add, 11
            async_call munchyMonkOne1
        endswitch
        break
    case 1
        random 1
        switch
        case 0
            0xB3 score_add, 13
            async_call munchyMonkTwo1
        endswitch
        break
    case 2
        random 1
        switch
        case 0
            0xB3 score_add, 12
            async_call munchyMonkThree1
        endswitch
        break
    endswitch
    rest 0x18
    input 1
stop

