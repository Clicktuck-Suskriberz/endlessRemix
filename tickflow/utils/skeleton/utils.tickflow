// Call checkXpress a beat early.

checkApress:
    0x201
    if_lt 0x103
        stop
    endif
    rest 0x28
    async_call waitApress
    label 0xAAAA
    0x200<2> 0
    if 1
        kill_loc waitApress
        goto 0xAAAE
    endif
    rest 1
    goto 0xAAAA
    label 0xAAAE
stop

waitApress:
    rest 16
    play_sfx 0x10002AF
    kill_loc checkApress
stop