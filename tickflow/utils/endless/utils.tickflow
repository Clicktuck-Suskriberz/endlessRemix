                set_func 3, nop
                set_func 2, loadSoumenLate
                set_func 1, checkA2
            endif
        break
        case 0x4A //Battle Start Melodii
            0xB2 9
            if 0x3C //Stop games from being played next to each other
                goto loadNextGame
            else
                0xB2 10
                if 1
                    0xB3 3, 1337
                    0xB3 0, 0x4A
                endif
                set_func 0, sequenceUpbeat
                async_call loadUpbeat
                set_func 3, nop
                set_func 2, loadUpbeatLate
                set_func 1, checkA2
            endif
        break
        case 0x4B //Groove Rush #1
            0xB2 9
            if 0xC //Stop games from being played next to each other
                goto loadNextGame
            else
                0xB2 10
                if 1
                    0xB3 3, 1337
                    0xB3 0, 0x4B
